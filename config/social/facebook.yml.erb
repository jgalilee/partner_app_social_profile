development:
  app_id: <%= ENV["FACEBOOK_APP_ID"] %>
  secret_key: <%= ENV["FACEBOOK_SECRET"] %>
  site_url: "https://graph.facebook.com/v2.3/"
  token_url: "/oauth/access_token"
  authorize_url: "https://www.facebook.com/dialog/oauth?client_id=%s&redirect_uri=%s"
  callback_url: "http://localhost:3000/sessions/facebook/callback"
