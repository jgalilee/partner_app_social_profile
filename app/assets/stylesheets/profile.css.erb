#profile-card-container {
  padding: 5% 0;
  font-family: "
}

#profile-card-container #profile-card-center {
  padding: 1% 0;
}

#profile-card {
  max-height: 610px;
  min-height: 610px;
}

#profile-card .logout-link {
  position: relative;
  float: right;
  padding: 10px;
  text-transform: uppercase;
  color: rgb(255,255,255);
  text-decoration: none;
  font-family: "Proxima Nova Alt Thin";
  font-size: 10pt;
  letter-spacing: 2px;
}

#profile-card,
#profile-card-title {
  margin: auto;
  max-width: 980px;
  min-width: 980px;
  margin-bottom: 46px;
  letter-spacing: 3.15px;
  font-size: 16pt;
  color: rgb(80, 227, 194);
}

#profile-card-title .profile-card-title-bold {
  font-family: "Proxima Nova Alt Bold";
}

#profile-card-title .profile-card-title-light {
  font-family: "Proxima Nova Alt Light";
}

#profile-card {
  border: solid 2px rgb(80, 227, 194);
  border-radius: 15px;
  color: rgb(255, 255, 255);
}

/* social basics  */

#profile-card .profile-social-basics {
  background: rgb(80, 227, 194);
  font-family: "Proxima Nova Alt Bold";
  padding: 24px;
  padding-left: 45px;
  border-radius: 13px 13px 0px 0px;
  min-height: 220px;
  max-height: 220px;
}

#profile-card .profile-social-basics .profile-social-basics-details {
  padding-left: 24px;
  font-size: 48px;
}

#profile-card .profile-social-basics img {
  width: 135px;
  border: solid 5px rgb(255,255,255);
  border-radius: 50%;
}

#profile-card hr {
  margin-top: 15px;
  padding: 0.5px;
  border-width: 0px;
  border-color: rgb(255,255,255);
  background-color: rgb(255,255,255);
}

#profile-card .profile-social-basics-image {
  float: left;
}

#profile-card .profile-social-basics-details {
  float: left;
  min-width: 540px;
}

#profile-card .profile-social-basics-details .information {
  font-size: 11pt;
  font-family: "Proxima Nova Alt Bold";
  letter-spacing: 2px;
  margin-bottom: 8px;
}

#profile-card .profile-social-basics-details .details {
  font-size: 11pt;
  font-family: "Proxima Nova Alt Light";
  margin-bottom: 10px;
}

/* salesforce.com */

#profile-card .profile-salesforce-basics {
  padding-top: 20px;
  padding-left: 30px;
  max-height: 290px;
  min-height: 290px;
  font-size: 13pt;
  color: rgb(170, 170, 170);
  font-family: "Proxima Nova Alt Light";
}


#profile-card .profile-salesforce-basics-title {
  font-family: "Proxima Nova Alt Bold";
  color: rgb(80, 227, 194);
  text-transform: uppercase;
  margin-bottom: 12px;
}

#profile-card .profile-salesforce-basics dl {
  padding: 1.5em;
}

#profile-card .profile-salesforce-basics dt {
  font-family: "Proxima Nova Alt Bold";
  float: left;
  clear: left;
  width: 100px;
  text-align: right;
  text-transform: uppercase;
}

#profile-card .profile-salesforce-basics dt:after {
  content: ":";
}

#profile-card .profile-salesforce-basics dd {
  margin: 0 0 0 110px;
  padding: 0 0 1.5em 0;
}

#profile-card .profile-salesforce-basics-left-side {
  float: left;
  min-width: 440px;
}

#profile-card .profile-salesforce-basics-right-side {
  float: left;
  min-width: 500px;
}

#profile-card .profile-salesforce-basics-right-side dt {
  width: 170px;
}

#profile-card .profile-salesforce-basics-right-side dd {
  margin: 0 0 0 180px;
}

#profile-card .profile-salesforce-basics .profile-salesforce-basics-controls .button {
  border: none;
  font-size: 12pt;
  float: right;
  padding: 15px;
  margin: 25px;
  background: rgb(80, 227, 194);
  border-radius: 10px;
  color: rgb(255,255,255);
  text-decoration: none;
  text-transform: uppercase;
  font-family: "Proxima Nova Alt Light";
  padding-left: 50px;
  padding-right: 50px;
}

#profile-card form input[type="text"] {
  margin: 0px;
  border: 1px solid rgb(80, 227, 194);
  padding: 4px;
  font-size: 13pt;
  letter-spacing: 3.15px;
  font-family: "Proxima Nova Alt Light";
  color: rgb(170,170,170);
  width: 100%;
  position: relative;
  top: -5px;
  left: -5px;
}

